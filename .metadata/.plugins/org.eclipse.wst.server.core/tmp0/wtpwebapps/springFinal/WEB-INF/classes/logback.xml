<?xml version="1.0" encoding="UTF-8"?>
<configuration debug="true">
	<appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
		<encoder>
			<pattern>%d %5p [%c] %m%n</pattern>
		</encoder>
	</appender>
	<logger name="com.myWeb.www" level="debug" appender-ref="STDOUT"></logger>
	<logger name="org.springframework" level="debug" appender-ref="STDOUT"></logger>
	<logger name="org.springframework.jdbc" level="debug" appender-ref="STDOUT"></logger>
	<logger name="jdbc.sqlonly" level="debug" appender-ref="STDOUT"></logger>
	<logger name="jdbc.resultsettable" level="debug" appender-ref="STDOUT"></logger>
	
	<root level="debug">
		<appernder-ref ref="STDOUT"/>
	</root>
</configuration>